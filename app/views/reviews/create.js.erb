document.querySelector("#close-review-form-modal").click()
document.querySelector("#review-form-modal").remove()
document.querySelector("#open-review-form-modal").remove()
document.querySelector("#race-rating").innerText = '<%= @review.race.rating %>'
document.querySelector("#review-swipe").insertAdjacentHTML("afterbegin",'<%= j render "review_box", review: @review %>')

document.querySelector("body").insertAdjacentHTML("beforebegin", '<%= j render partial: "shared/flash_messages", absolute: @absolute %>')
var element = document.querySelector("#flash_messages")
var width = element.offsetWidth
element.style.top = (window.pageYOffset + window.innerHeight / 2.5) + "px"
element.style.right = ((window.innerWidth -  width) / 2) + "px"
